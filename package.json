{
  "name": "is-bot-down-bot",
  "version": "1.0.0",
  "description": "Telegram bot that tracks the down status of other bots",
  "main": "dist/app.js",
  "repository": "https://github.com/backmeupplz/IsBotDownBot",
  "author": "backmeupplz <backmeupplz@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "distribute": "node dist/app.js",
    "develop": "concurrently -k -i -p \"[{name}]\" -n \"Node,TypeScript\" -c \"yellow.bold,cyan.bold\" \"yarn watch-js\" \"yarn watch-ts\"",
    "build-ts": "tsc --skipLibCheck",
    "watch-ts": "tsc -w --skipLibCheck",
    "watch-js": "nodemon dist/app.js",
    "pretty": "prettier --check src",
    "lint": "yarn pretty && eslint --max-warnings 0 --ext ts,tsx,json src"
  },
  "dependencies": {
    "@grammyjs/i18n": "^0.3.0",
    "@grammyjs/runner": "^1.0.2",
    "@hapi/boom": "^9.1.4",
    "@koa/cors": "^3.1.0",
    "@typegoose/typegoose": "^9.2.0",
    "amala": "^7.0.0",
    "axios": "^0.24.0",
    "crypto-js": "^4.1.1",
    "grammy": "^1.3.4",
    "input": "^1.0.1",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.1.1",
    "lodash": "^4.17.21",
    "mongoose": "6.4.6",
    "mongoose-findorcreate": "^3.0.0",
    "telegram": "^1.9.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.0.2",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.4",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "concurrently": "^6.3.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
